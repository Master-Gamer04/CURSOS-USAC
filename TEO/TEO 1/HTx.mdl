PRESTAMO D1= INTEG (
	INTERES D1,
		2000)
	~	Dolares
	~		|

PRESTAMO D2= INTEG (
	INTERES D2,
		1000
		)
	~	Dolares
	~		|

INTERES D1=
	PRESTAMO D1*((1-(1+PRESTAMO D1*TASA D1)^(-12))/PRESTAMO D1*TASA D1)-PRESTAMO D1*TASA D1
	~	Dolares/Month
	~		|

INTERES D2=
	PRESTAMO D2*((1-(1+PRESTAMO D2*TASA D2)^(-12))/PRESTAMO D2*TASA D2)-PRESTAMO D2*TASA D2
	~	Dolares/Month
	~		|

TASA D1=
	0.02
	~	
	~		|

TASA D2=
	0.04
	~	
	~		|

INTERES=
	PRESTAMO*((1-(1+PRESTAMO*TASA)^(-12))/PRESTAMO*TASA)-PRESTAMO*TASA
	~	Dolares/Month
	~		|

INTERES B=
	PRESTAMO B*((1-(1+PRESTAMO B*TASA B)^(-12))/PRESTAMO B*TASA B)-PRESTAMO B*TASA B
	~	Dolares/Month
	~		|

PRESTAMO= INTEG (
	INTERES,
		2000)
	~	Dolares
	~		|

PRESTAMO B= INTEG (
	INTERES B,
		1000
		)
	~	Dolares
	~		|

TASA=
	0.05
	~	
	~		|

TASA B=
	0.1
	~	
	~		|

